<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.cyy.dao.IUserDao">
    <!--配置查询所有-->
    <select id="findAll" resultType="com.cyy.vo.UserVO">
        select
         id,
         username,
         birthday,
         sex,
         address
         from user
    </select>
</mapper>